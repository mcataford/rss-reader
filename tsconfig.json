{
    "compilerOptions": {
        "target": "es2017",
        "jsx": "react-jsx",
        "jsxImportSource": "react",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "rootDir": ".",
        "paths": {
            "@/*": ["./src/*"]
        }
    },
    "include": ["src/**/*", "netlify/**/*"]
}

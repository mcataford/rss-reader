{
  "name": "rss-reader",
  "private": true,
  "version": "1.0.0",
  "packageManager": "yarn@4.1.0",
  "license": "GPL-3.0",
  "scripts": {
    "lint": "yarn biome format src/*.ts && yarn biome check src/*.ts",
    "lint:fix": "yarn biome format src/*.ts --write && yarn biome check src/*.ts --apply",
    "types": "tsc --noEmit",
    "clean": "rm -rf dist/*",
    "start": "netlify dev",
    "start:parcel": "parcel serve src/index.html --port 8080",
    "build": "parcel build src/index.html",
    "build:watch": "parcel watch src/index.html",
    "build:bundlesize": "parcel build src/index.html --reporter @parcel/reporter-bundle-analyzer"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.10",
    "@mui/material": "^5.15.10",
    "htmlparser2": "8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "3.39.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@parcel/reporter-bundle-analyzer": "^2.9.3",
    "@parcel/validator-typescript": "^2.9.3",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "esbuild": "^0.17.3",
    "jest": "29.3.1",
    "netlify-cli": "^15.7.0",
    "parcel": "^2.9.3",
    "typescript": "^4.9.4"
  }
}

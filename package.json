{
  "name": "rss-reader",
  "private": true,
  "version": "1.0.0",
  "packageManager": "yarn@4.1.0",
  "license": "GPL-3.0",
  "scripts": {
    "lint": "yarn biome format src/*.ts && yarn biome check src/*.ts",
    "lint:fix": "yarn biome format src/*.ts --write && yarn biome check src/*.ts --apply",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist/*",
    "start": "netlify dev",
    "start:app": "vite ./src --config ./vite.config.js --port 8080",
    "build": "vite build ./src --config ./vite.config.js",
    "build:watch": "vite build watch ./src --config ./vite.config.js"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@mui/icons-material": "^5.15.10",
    "@mui/material": "^5.15.10",
    "htmlparser2": "9.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "3.39.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-basic-ssl": "^1.1.0",
    "@vitejs/plugin-legacy": "^5.3.0",
    "jest": "29.3.1",
    "netlify-cli": "^17.16.2",
    "terser": "^5.27.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.3"
  }
}

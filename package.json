{
  "name": "rss-reader",
  "private": true,
  "version": "1.0.0",
  "packageManager": "yarn@3.3.1",
  "license": "GPL-3.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start:app": "yarn workspace app start",
    "lint": "eslint packages",
